type User {
  id: ID! @id
  nick: String!
}

type Round {
  id: ID! @id
  word1: String!
  word2: String!
}

type Lobby {
  id: ID! @id
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
  started: Boolean!
  creator: ID! 
  partner: ID  
  creator_word: String
  partner_word: String
  rounds: [Round!]
}